<h1>Details of your stay</h1>
<%= simple_form_for [@property, @booking] do |f| %>
  <%= f.input :start_date, placeholder: 'Start date' %>
  <%= f.input :end_date, placeholder: 'End date'%>
  <%= f.input :number_of_guests, placeholder: 'How many people'%>
  <%= f.submit class: "btn btn-flat" %>
<% end %>
